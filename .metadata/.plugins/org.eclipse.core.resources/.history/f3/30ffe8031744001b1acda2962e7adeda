package day02;

public class Test04 {
	public static void main(String[] args) {
		int x=100, y=7;
		System.out.println(x/y);
		System.out.println(x%y);
		
		int num=100;
		System.out.println(++num);
		System.out.println(num);
		
		
		String msg=null;
//				"Hello Java";
		
		System.out.println(msg.toUpperCase());
		System.out.println("END");
		return;
	}

}
