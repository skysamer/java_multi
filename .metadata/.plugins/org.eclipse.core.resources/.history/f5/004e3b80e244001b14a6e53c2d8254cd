package day03;

public class Test04 {

	public static void main(String[] args) {
		int num=99;
		int i;
		for(i=0; i<10; i+=2) {
			System.out.println("Hello java "+i);
		}
		System.out.println(i);
		System.out.println("------------------------------------------");
		
		for(int count=9; count>10; count--) {
			System.out.println("html5");
		}
		
		for(int count=0; ; count++) {
			System.out.println("~~~~~");
			if(count == 4) break;
		}
		System.out.println("----------------------------------------------");
		
		i=0;
		while(i<5) {
			System.out.println("while loop "+i);
			i++;
		}

	}

}
