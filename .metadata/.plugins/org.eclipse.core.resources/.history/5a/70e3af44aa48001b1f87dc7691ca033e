package day04;

public class Test01 {

	public static void main(String[] args) {
		int[] jumsu = null;
		jumsu=new int[5];
		
		jumsu[0]=99;
		jumsu[1]=100;
		jumsu[2]=89;
		jumsu[3]=99;
		jumsu[4]=79;
		
		int sum =0;
		for(int i=0; i<jumsu.length; i++) {
			sum+=jumsu[i];
			System.out.println(jumsu[i]);
		}
		
		System.out.println(sum/jumsu.length);
			
		
		
		

	}

}
