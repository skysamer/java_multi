package day04;

public class Test03 {
	public static void main(String[] args) {
		int[]num1={66, 77, 33, 55, 100};  // new int[] 생략가능, 라인 분리시 추가해야됨
		int num2[]=num1;
		int[] num3=num2.clone();
		
		num1[0]=11;
		num2[0]=22;
		num3[0]=33;
		
		num1=null;
		num2=null;   
		System.out.println("-----------------------------------------------");

	}

}
