package day02;

public class dsadsa {

	public static void main(String[] args) {
		System.out.println(1+2);
		System.out.println('1'+'2');
		System.out.println("1"+"2");
		System.out.println("~~ "+1+2);
		System.out.println(1+2+"~~");
		System.out.println("Hello"+"java"+'!');
		 double d = 1.4e3f;


	}

}
