package day05;

public class Calc {
	// +, -, *, /
	// 두 수를 더하는 add함수 정의
	public int add(int a, int b) {
		return a+b;
	}
	
	public int sub(int a, int b) {
		return a-b;
	}
	
	public int multi(int a, int b) {
		return a*b;
	}
	
	public int div(int a, int b) {
		if(b!=0) return a/b;
		else return 0;
	}

}
