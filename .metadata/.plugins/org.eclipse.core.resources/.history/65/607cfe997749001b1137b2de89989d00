package day04;

public class Employee {
	// 데이터+기능
	private String name; //이름
	private String dept; //부서
	private int age; //나이
	
	public void setAge(int age){
		if(age>0)
		this.age=age;
	}
	public int getAge(){
		return age;
	}
	
	
	public void print () {//출력
		System.out.printf("[사원명:%s, 근무부서:%s, 나이:%d] %n", this.name, this.dept, this.age);
	}

}
