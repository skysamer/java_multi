package day04;

public class Test03 {
	public static void main(String[] args) {
		int[]num1={66, 77, 33, 55, 100};  // new int[] 생략가능, 라인 분리시 추가해야됨
		int num2[]=num1;
		int[] num3=num2.clone();
		
		if(5<num1.length)num1[5]=11;
		num2[0]=22;
		num3[0]=33;
		
		
		num1=null;
		num2=null;   
		
		System.out.println("-----------------------------------------------");

		
		int[] num4={5, 9, 1, 4, 3};
		int[] num5=new int[num4.length*2];
		
		System.arraycopy(num4, num4[0], num5, num5[0], num4.length);
		
		for(int i=0; i<num5.length; i++) {
			System.out.printf("%d ", num5[i]);
		}

		System.out.println("-----------------------------------------------");
	}

}
