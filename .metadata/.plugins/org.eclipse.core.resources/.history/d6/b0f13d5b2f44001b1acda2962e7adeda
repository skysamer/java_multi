package day02;

public class Prob2_이상민 {
	public static void main(String[] args) {
		int num=-90;
		String name1=num > 0 ? "양수" :  (num < 0 ? "음수" : "0");
		System.out.println(num+": "+name1);
		
		char ch='A';
		if((int)ch>=65 && (int)ch<=90) {
			System.out.println(ch+": "+(char)(ch+32));
		}
		else {
			System.out.println(ch+": "+ch);
		}
	}

}
