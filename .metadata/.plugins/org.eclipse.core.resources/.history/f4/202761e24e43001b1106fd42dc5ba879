package day01;

class Test02 {
	// 클래스의 구성 요소 = 데이터(변수) + 기능(함수)
	
	public static void main(String[] args) {
		// 명령문, Local 변수  
		System.out.println(30);
		System.out.println(30.6);
		System.out.println(true);
		System.out.println(false);
		System.out.println("홍길동");
		System.out.println('A');
	}

}
