package day02;

public class Prob2 {

	public static void main(String[] args) {
		int num=-90;
		System.out.println((num > 0 ? "양수" :  (num < 0 ? "음수" : "0"));
		
		char ch='P';
		if((int)ch>=65 && (int)ch<=90) {
			System.out.println((char)(ch+32));
		}
		else {
			System.out.println(ch);
		}
	}

}
